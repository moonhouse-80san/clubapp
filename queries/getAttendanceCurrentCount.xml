<?xml version="1.0" encoding="utf-8"?>
<query id="getAttendanceCurrentCount" action="select">
    <tables>
        <table name="clubapp_attendance_current" />
    </tables>
    <columns>
        <column name="count(*)" as="count" />
    </columns>
    <conditions>
        <condition operation="equal" column="member_id" var="member_id" />
        <condition operation="equal" column="attendance_date" var="attendance_date" />
    </conditions>
</query>