<?xml version="1.0" encoding="utf-8"?>
<query id="getMemberAwards" action="select">
    <tables>
        <table name="clubapp_member_awards" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="member_id" var="member_id" />
        <condition operation="order" column="display_order" order="asc" />
    </conditions>
</query>