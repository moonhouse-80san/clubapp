    <column name="member_id" type="number" size="11" notnull="true"/>
    <column name="payment_date" type="date" notnull="true"/>
    <column name="amount" type="number" size="11" notnull="true" default="0"/>
    <column name="regdate" type="date"/>
    <index name="idx_member_id" columns="member_id"/>
    <index name="idx_payment_date" columns="payment_date"/>
</table>